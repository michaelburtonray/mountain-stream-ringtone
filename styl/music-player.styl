section#music-player {
  background: oldLace;
  position: relative;
  height: 100%;

  iframe {
    position: absolute;
    top: 50%;
    left: 0;
    width: 72%;
  }

  img {
    width: 28%;
    top: 50%;
    position: absolute;
    right: 0;
    transform: translateY(-50%);
  }

}


.g-background-default {
  background: transparent !important;
}